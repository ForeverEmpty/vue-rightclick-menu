{
  "name": "@foreverempty_/vue-rightclick-menu",
  "version": "1.0.2",
  "description": "A right-click menu implemented using Vue3",
  "type": "module",
  "main": "./dist/umd/index.umd.cjs",
  "module": "./dist/es/index.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/es/index.js",
      "require": "./dist/umd/index.umd.cjs"
    },
    "./dist/": {
      "import": "./dist/",
      "require": "./dist/"
    }
  },
  "sideEffects": [
    "./dist/index.css"
  ],
  "scripts": {
    "build": "run-p build-es build-umd",
    "build-es": "vite build --config vite.es.config.ts",
    "build-umd": "vite build --config vite.umd.config.ts",
    "dev": "pnpm --filter @vue-rightclick-menu/play dev",
    "test": "vitest --coverage"
  },
  "workspaces": [
    "packages/*"
  ],
  "keywords": [
    "vue3",
    "vue",
    "rightclick"
  ],
  "author": "ForeverEmpty_",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ForeverEmpty/vue-rightclick-menu"
  },
  "packageManager": "pnpm@10.16.1",
  "dependencies": {
    "@vue-rightclick-menu/components": "workspace:*",
    "@vue-rightclick-menu/hooks": "workspace:*",
    "@vue-rightclick-menu/theme": "workspace:*",
    "@vue-rightclick-menu/utils": "workspace:*",
    "vue": "^3.5.22"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-svg-core": "^7.1.0",
    "@fortawesome/free-solid-svg-icons": "^7.1.0",
    "@fortawesome/vue-fontawesome": "^3.1.2",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^24.9.1",
    "@types/shelljs": "^0.8.17",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitejs/plugin-vue-jsx": "^5.1.1",
    "@vitest/coverage-v8": "3.2.4",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "jsdom": "^27.0.1",
    "lodash-es": "^4.17.21",
    "npm-run-all": "^4.1.5",
    "postcss-color-mix": "^1.1.0",
    "postcss-each": "^1.1.0",
    "postcss-each-variables": "^0.3.0",
    "postcss-for": "^2.1.1",
    "postcss-nested": "^7.0.2",
    "resize-observer-polyfill": "^1.5.1",
    "shelljs": "^0.10.0",
    "typescript": "~5.9.3",
    "vite": "^7.1.11",
    "vite-plugin-compression2": "^2.3.0",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.1.1"
  }
}
